<h1>Listing movie_tags</h1>

<table>
  <thead>
    <tr>
      <th>Movie</th>
      <th>Tag</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @movie_tags.each do |movie_tag| %>
      <tr>
        <td><%= movie_tag.movie_id %></td>
        <td><%= movie_tag.tag_id %></td>
        <td><%= movie_tag.name %></td>
        <td><%= link_to 'Show', movie_tag %></td>
        <td><%= link_to 'Edit', edit_movie_tag_path(movie_tag) %></td>
        <td><%= link_to 'Destroy', movie_tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Movie tag', new_movie_tag_path %>
